    "release:version": "tsx scripts/version.ts",
    "release:package": "tsx scripts/package.ts",
    "release:ota": "tsx scripts/ota.ts",
    "release:hotpatch": "tsx scripts/hotpatch.ts"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.23.1",
    "@starter/native-example": "*",
    "expo": "~51.0.0",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~3.3.5",
    "expo-localization": "~15.0.1",
    "expo-router": "^4.0.0",
    "expo-updates": "~0.26.9",
    "react": "18.2.0",
    "react-i18next": "^14.1.0",
    "react-native": "0.74.1",
    "react-native-safe-area-context": "^4.10.5",
    "react-native-screens": "^3.31.1",
    "swr": "^2.2.5",
    "zustand": "^4.5.5",
    "i18next": "^23.12.2"
