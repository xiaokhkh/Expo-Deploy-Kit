{
  "name": "expo-router-i18n-starter",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.18.2",
  "workspaces": [
    "modules/*"
  ],
  "main": "./entry.js",
  "scripts": {
    "start": "expo start",
    "dev": "expo start",
    "dev:client": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "typecheck": "tsc --noEmit",
    "release": "tsx scripts/release.ts",
    "release:version": "tsx scripts/version.ts",
    "release:package": "tsx scripts/package.ts",
    "release:update": "tsx scripts/hotpatch.ts"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@starter/in-app-update": "workspace:*",
    "@starter/native-example": "workspace:*",
    "expo": "~54.0.0",
    "expo-constants": "~18.0.13",
    "expo-dev-client": "~6.0.20",
    "expo-linking": "~8.0.11",
    "expo-localization": "~17.0.8",
    "expo-router": "~6.0.21",
    "expo-updates": "~29.0.16",
    "i18next": "^23.12.2",
    "@formatjs/intl-pluralrules": "^6.1.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "^14.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "^0.21.2",
    "react-native-worklets": "0.7.1",
    "swr": "^2.2.5",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@react-native-community/cli": "^20.1.0",
    "@types/react": "~19.1.10",
    "tsx": "^4.16.2",
    "typescript": "~5.3.3"
  }
}
