{
  "name": "expo-router-i18n-starter",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "modules/*"
  ],
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "dev": "expo start",
    "dev:client": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "typecheck": "tsc --noEmit",
